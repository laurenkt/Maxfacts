{
  "name": "maxfacts",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "DEBUG=maxfacts nodemon --exec babel-node -- ./bin/www",
    "backup": "mongodump -d maxfacts -o ./backups/$(date \"+%Y-%m-%d-%Hh-%Mm\")",
    "restore": "mongo maxfacts --eval \"db.dropDatabase();\"; cd backups/$(command ls -t backups | head -1); mongorestore -d maxfacts maxfacts",
    "test": "TEST=maxfacts nyc --reporter=html --reporter=text mocha --compilers js:babel-register",
    "test:quick": "TEST=maxfacts nyc --reporter=html --reporter=text mocha --grep integrations --invert --compilers js:babel-register",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run flow && npm run test:quick",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "chai": "^3.5.0",
    "connect-ensure-login": "^0.1.1",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "domutils": "^1.5.1",
    "dotenv": "^2.0.0",
    "express": "~4.13.4",
    "express-session": "^1.14.0",
    "mocha": "^3.2.0",
    "mongodb": "^2.2.4",
    "mongoose": "^4.5.7",
    "morgan": "~1.7.0",
    "multer": "^1.1.0",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "sanitize-html": "github:comaleaf/sanitize-html#filter-contexts",
    "serve-favicon": "~2.3.0",
    "sinon": "^1.17.7"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify-middleware": "^7.0.0",
    "chalk": "^1.1.3",
    "eslint": "^3.2.2",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.0.0",
    "express-handlebars": "^3.0.0",
    "flow-bin": "^0.38.0",
    "ghooks": "^2.0.0",
    "html-minifier": "^3.2.3",
    "immutable": "^3.8.1",
    "js-beautify": "^1.6.4",
    "jsx-control-statements": "^3.1.2",
    "lodash": "^4.14.1",
    "mongodb": "^2.2.16",
    "mongoose": "^4.8.1",
    "node-cli": "^0.1.0",
    "node-sass-middleware": "0.8.0",
    "nodemon": "^1.9.2",
    "nyc": "^10.1.2",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "slate": "^0.16.11",
    "supertest": "^3.0.0"
  }
}
